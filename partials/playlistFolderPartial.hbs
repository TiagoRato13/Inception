<a href="/playlist/{{_id}}" id="{{_id}}">
  <div class="card">
    <div>
      <form action="/playlist/{{_id}}/edit">
        <button type="submit" class="card-edit">Edit playlist</button>
      </form>
      <form action="/playlist/{{_id}}/delete" method="post">
        <button type="submit" class="card-delete"></button>
      </form>
      <img src="{{image}}" alt="{{name}}" class="image-icon" />
    </div>
    <br />
    <h3 class="card-title">{{name}}</h3>

  </div>
</a>