<a href="/create-playlist"><button>Create playlist</button></a>

<footer>
  <div id="content">
    <div id="range">
      <input type="range" id="range-val" value="46" min="0" max="143" />
      <div id="tip">Some tip</div>
    </div>
    <div id="time">
      <p>0:46</p>
    </div>
    <div id="buttons">
      <i class="fa fa-repeat"></i>
      <i class="fa fa-step-backward fa-2x"></i>
      <i class="fa fa-pause fa-2x step"></i>
      <i class="fa fa-step-forward fa-2x"></i>
      <i class="fa fa-random"></i>
    </div>
    <div id="total-time">
      <p>2:22</p>
    </div>
  </div>
</footer>